public class Link {
     public Link(String Name, int StudentNo, String course, String Address, String Email) {
        this.Name = Name;
        this.StudentNo = StudentNo;
        this.course = course;
        this.Year = Year;
        this.Email = Email;

    }

    String Name;
    int StudentNo;
    String Year;
    String course;
    String Email;
    
    Link Previous;
    Link Next;
}
